{
  "name": "bookmanagementTEST",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "npm-run-all test build server",
    "postbuild": "npm run server",
    "test:watch": "npm run test -- --watch",
    "prebuild": "npm-run-all test clean",
    "clean": "rimraf ./lib && mkdir lib",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "server": "cross-env NODE_ENV=dev PORT=3001 node lib/bin/www",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/bm-test.js",
    "coverage": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/bm-test.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.3",
    "koa-jwt": "^3.5.1",
    "mongo": "^0.1.0",
    "mongo-uri": "^0.1.2",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.3.6",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "chai-things": "^0.2.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  }
}
